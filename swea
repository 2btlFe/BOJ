#include <bits/stdc++.h>
#define ll long long

using namespace std;

const ll mod = 1e9+7;

void solve() {
    string s; cin >> s;
    ll dp[10001][16];
    memset(dp, 0, sizeof(dp));
    
    int n = s.size();
    
    for(int i = 0; i < 16; i++) {
        if(i & 1) dp[0][i] = 1; 
    }
    
    for(int i = 1; i <= n; i++) {
        for(int j = 1; j < 16; j++) {
            
            if((j & (1 << (s[i - 1] - 'A'))) == 0) continue;
            
            for(int k = 1; k < 16; k++) {
                int ok = j & k;
                if(ok != 0 && dp[i-1][k] != 0) {
                    dp[i][j] = (dp[i][j] + dp[i-1][k]) % mod;
                }
            }    
        }
    }
    
    ll ret = 0;
    for(int i = 0; i < 16; i++) {
        cout << dp[n][i] << endl;
        ret = (ret + dp[n][i]) % mod;
    }
    cout << ret << endl;
}

int main() {
    int tt; cin >> tt;
    for(int t = 1; t <= tt; t++) {
        cout << "#" << t << " ";
        solve();
    }
}
